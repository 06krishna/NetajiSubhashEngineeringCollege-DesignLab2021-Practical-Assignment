<h1></h1>

{{# each products }}
  <div class="row">
    <div class="col-xs">
      <div class="thumbnail">
        <div class="caption">
          <h3>{{ this.title }} </h3>
          <div class="col">
            <div class="price pull-left"><i  aria-hidden="true"></i> {{ this.price }} rs</div>
              <a href="/add/{{ this.id }}" class="btn btn-primary" role="button"> Add to cart</a>
            </div>
          </div>
        </div>
      </div>
{{/each}}